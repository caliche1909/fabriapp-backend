{
  "name": "fabriapp-backend",
  "version": "1.0.0",
  "description": "Backend API para FabriApp - Sistema de gestión de rutas y geolocalización",
  "main": "src/server.js",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node src/server.js",
    "dev": "NODE_ENV=development nodemon src/server.js",
    "dev:local": "NODE_ENV=development nodemon src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "health": "curl -f http://localhost:3000/health || exit 1",
    "docker:build": "docker build -t fabriapp-backend .",
    "docker:run": "docker run -p 3000:3000 fabriapp-backend",
    "docker:health": "docker inspect --format='{{.State.Health.Status}}' $(docker ps -q --filter ancestor=fabriapp-backend)"
  },
  "keywords": [
    "api",
    "express",
    "nodejs",
    "geolocation",
    "websocket",
    "backend"
  ],
  "author": "Carlos Andres Moran Caicedo <karyngadm@gmail.com> (https://github.com/caliche1909)",
  "license": "UNLICENSED",
  "devDependencies": {
    "nodemon": "^3.1.9",
    "sequelize-auto": "^0.8.8",
    "sequelize-cli": "^6.6.2"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.6.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-form-data": "^2.0.23",
    "express-rate-limit": "^7.5.1",
    "googleapis": "^159.0.0",
    "imagemin": "^9.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sharp": "^0.34.2",
    "socket.io": "^4.8.1"
  }
}
